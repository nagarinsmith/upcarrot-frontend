  },
  "devDependencies": {
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "babel-plugin-import": "^1.11.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "concurrently": "^5.0.0",
    "customize-cra": "^0.2.9",
    "electron": "^7.1.1",
    "electron-builder": "^21.2.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-config-react-app": "^5.0.2",
    "eslint-plugin-flowtype": "2.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "1.5.0",
    "husky": ">=1",
    "lint-staged": ">=8",
    "prettier": "1.17.0",
    "react-app-rewire-sass-modules": "^1.1.7",
    "react-scripts": "^3.2.0",
    "wait-on": "^3.3.0"
  },
  "main": "public/electron.js",
  "scripts": {
    "electron-dev": "concurrently \"yarn workspace desktop start\" \"wait-on http://localhost:3000 && electron .\"",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "test:cov": "react-app-rewired test --coverage",
    "test:debug": "react-app-rewired --inspect-brk test --runInBand",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "eject": "react-app-rewired eject"
  },
  "eslintConfig": {
    "extends": "react-app",
    "parserOptions": {
      "ecmaFeatures": {
        "legacyDecorators": true
      }
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 9",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/*index.js",
      "!src/serviceWorker.js",
      "!src/polyfill.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "@shared": "../components"
}
